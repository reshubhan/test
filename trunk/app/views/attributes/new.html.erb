<div class="content-box">
  <div class="content-box-header">
    <h3 style="cursor: s-resize;">
      Add Attribute
    </h3>
    <div style="float: right;margin: 10px;"><%= link_to 'Back', attributes_path, :class => "button" %></div>
    <div class="clear"></div>
  </div>
  <div class="content-box-content">
    <div style="display: block;" class="tab-content default-tab scrollDiv">
      <% form_for(@attribute) do |f| %>
        <%= f.error_messages %>
        <%= render :partial => "form", :locals => {:f => f} %>
        <p>
          <%= f.submit "Submit", :class => "button", :onclick => "return validate_checkboxes();" %>
        </p>
      <% end %>
    </div>
  </div>
</div>
<div class="content-box">
  <div class="content-box-header">
    <h3 style="cursor: s-resize;">
      <% if @asset_type %>
        Listing Asset Types
      <% else %>
        Listing Assets
      <% end %>
    </h3>
    <div style="float: right;margin: 10px;"><%= button_to 'Create New Asset', new_asset_path, :class => "button" %></div>
    <div class="clear"></div>
  </div>
  <div class="content-box-content">
    <div style="display: block;" class="tab-content default-tab scrollDiv">
      <div id="results">
        <%= render :partial => "results" %>
      </div>
    </div>
  </div>
</div>





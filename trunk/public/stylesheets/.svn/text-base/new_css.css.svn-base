/* new menu */

.chromestyle{
    width: 758px;
    height: 35px;
    float: left;
    background: url("../images/menu_bg.gif") repeat-x;
}
.chromestyle:after{ /*Add margin between menu and rest of content in Firefox*/
                    content: ".";
                    display: block;
                    height: 0;
                    clear: both;
                    visibility: hidden;
}
.chromestyle ul{
    margin: 0;
    padding:0px;
    text-align: left; /*set value to "left", "center", or "right"*/
}
.chromestyle ul li{
    display: inline;
    display:block;
    float:left;
}
.chromestyle ul li a{
    /*color: #262626;*/
    color: #686767;
    font-weight: bold;
    display:block;
    float:left;
    padding:7px 7px 7px 7px;
    margin: 0;
    text-decoration: none;
    position:relative;
    /*z-index:1001;*/
    margin-top:3px;
}
.chromestyle ul li.navDiv{
    height:28px;

}
.chromestyle ul li a:hover,.chromestyle ul li a.selected,.chromestyle ul li a:active{ 
    background-color:#efefef;
    float: left;
    padding:7px 0px 10px 0px;
    margin-top:3px;
}
.chromestyle ul li a.actBx{
    background-color:#efefef;
    float: left;
    padding:7px 0px 7px 0px;
    margin-top:3px;
}

/* ######### Style for Drop Down Menu ######### */

.dropmenudiv{
    position:absolute;
    top: 0;
    line-height:18px;
    z-index:100;
    background-color: #efefef;
    visibility: hidden;
    margin-top:-2px;
    margin-left: -7px;
    padding: 5px 0px 0px 0px;
}
.dropmenudiv a{
    min-width:80px;
    display: inline;
    font-size:10px;
    font-weight: bold;
    color: #000000;
    padding: 0px 0px 0px 0px;
    float: left;
    text-align: center;
}
.dropmenudiv a:hover{
    /*min-width:80px;
	display: inline;
	font-size:10px;
	font-weight: bold;
	color: #000000;
	padding: 0px 0px 0px 0px;
	float: left;
	height:17px;
	background: url("../images/dropdown_selected.gif") no-repeat;*/
    -moz-background-clip:border !important;
    -moz-background-inline-policy:continuous !important;
    -moz-background-origin:padding !important;
    background:#e6e5e5 url(../images/dd_selected.gif) repeat-x scroll left top !important;
    border-color:#e6e5e5 !important;
    text-align: center;
    -moz-border-radius-bottomleft:4px;
    -moz-border-radius-bottomright:4px;
    -moz-border-radius-topleft:4px;
    -moz-border-radius-topright:4px;
}
* html .dropmenudiv a{ /*IE only hack*/
                       width: 100%;
                       font-size:10px;
                       font-weight: bold;
                       color: #000000;
}
.dropmenudiv a:hover{ /*THEME CHANGE HERE*/
                      font-size:10px;
                      font-weight: bold;
                      color: #000000;
                      text-decoration: none;
}
